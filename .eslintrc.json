{
  "extends": "@appium/eslint-config-appium",
  "overrides": [
    {
      "files": "test/**/*.js",
      "rules": {
        "func-names": "off"
      }
    },
    {
      "files": ["**/*.{mjs,cjs,js}"],
      "parserOptions": {"sourceType": "module"},
      "rules": {
        "@typescript-eslint/no-var-requires": "off"
      }
    }
  ]
}
